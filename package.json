{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "nuxi dev",
    "dev:tunnel": "nuxi dev --tunnel",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "vue-tsc --noEmit",
    "prepare": "nuxi prepare",
    "check:all": "pnpm run lint && pnpm run format:check && pnpm run test:types"
  },
  "dependencies": {
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui": "^3.1.3",
    "dompurify": "^3.2.6",
    "gsap": "^3.12.7",
    "marked": "^15.0.7",
    "medium-zoom": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@nuxtjs/i18n": "^9.5.4",
    "@sindresorhus/slugify": "^2.2.1",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.15.21",
    "@vueuse/nuxt": "^13.3.0",
    "eslint": "^9.27.0",
    "jsdom": "^26.0.0",
    "nuxt": "^3.17.4",
    "nuxt-kql": "^1.5.4",
    "nuxt-lazy-load": "^3.0.4",
    "prettier": "^3.5.3",
    "sitemap": "^8.0.0",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.2.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
